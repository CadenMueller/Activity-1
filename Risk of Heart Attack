Gender = str(input('Enter your gender Male or Female: '))
Age = int(input('Enter your age: '))
Total_Cholesterol = int(input('Enter your total cholesterol: '))
Smoker = str(input('Do you smoke, Yes or No: '))
HDL = int(input('Enter your HDL: '))
Treat = str(input('Is your systolic BP treated, Yes or No: '))
Systolic_BP = int(input('Enter your systolic BP: '))
Point_Total = 0
if Gender == 'Male':
    if Age >= 20 and Age < 35:
        Point_Total -= 9
        if Total_Cholesterol >= 160 and Total_Cholesterol < 200:
            Point_Total += 4 
        if Total_Cholesterol >= 200 and Total_Cholesterol < 240:
            Point_Total += 7
        if Total_Cholesterol >= 240 and Total_Cholesterol < 280:
            Point_Total += 9
        if Total_Cholesterol >= 280:
            Point_Total += 11
        if Smoker == 'Yes' :
            Point_Total += 8 
    if Age >= 35 and Age < 40:
        Point_Total -= 4
        if Total_Cholesterol >= 160 and Total_Cholesterol < 200:
